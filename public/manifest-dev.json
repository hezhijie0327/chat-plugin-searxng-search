{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "api": [
    {
      "description": "使用 SearXNG 进行搜索的开源元搜索引擎",
      "name": "searxngSearch",
      "parameters": {
        "properties": {
          "language": {
            "description": "搜索语言",
            "type": "string"
          },
          "q": {
            "description": "搜索的文本内容",
            "type": "string"
          },
          "time_range": {
            "description": "搜索的时间范围",
            "enum": [
              "day",
              "month",
              "year"
            ],
            "type": "string"
          }
        },
        "required": [
          "q"
        ],
        "type": "object"
      },
      "url": "http://localhost:3000/api/search"
    }
  ],
  "author": "hezhijie0327",
  "gateway": "http://localhost:3000/api/gateway",
  "homepage": "https://github.com/hezhijie0327/lobechat-chat-plugin-searxng",
  "identifier": "searxng-search",
  "meta": {
    "avatar": "https://raw.githubusercontent.com/searxng/searxng/master/src/brand/searxng-wordmark.svg",
    "description": "使用 SearXNG 进行隐私友好的元搜索",
    "tags": [
      "web",
      "search",
      "metasearch"
    ],
    "title": "SearXNG Search"
  },
  "settings": {
    "properties": {
      "SEARXNG_INSTANCE_URL": {
        "description": "您的 SearXNG 实例的 URL",
        "format": "url",
        "title": "SearXNG Instance URL",
        "type": "string"
      }
    },
    "required": [
      "SEARXNG_INSTANCE_URL"
    ],
    "type": "object"
  },
  "version": "1"
}

{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "automerge": true,
  "dependencyDashboard": true,
  "ignoreDeps": [],
  "labels": ["dependencies"],
  "postUpdateOptions": ["yarnDedupeHighest"],
  "prConcurrentLimit": 30,
  "prHourlyLimit": 0,
  "rebaseWhen": "conflicted",
  "schedule": "on sunday before 6:00am",
  "timezone": "UTC"
}
